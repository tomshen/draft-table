<div class="proposal active" id="<%= 'proposal' + _id %>">
	<div class="proposal-toggle toggleable"></div>
	<div class="proposal-title toggleable"><% if(locals.title) { title } else { "Student Proposal" } %></div>
	<div class="proposal-supporters toggleable"><% if(locals.supporters) { supporters.length } else { 0 } %> supporter </div>
	<div class="proposal-author"><% if(locals.author) { author } else { "" } %></div>
	<div class="proposal-extras">
		<div class="proposal-content">
			<% if(locals.elements) { elements.forEach(function (el, index) { %>
				<%- partial('partials/' + el.type, el) %>
			<% }); }%>
		</div>

		
	<div class="button supporters-button proposal-support" data-proposal-id="<%= _id %>">support this proposal</div>

		<div class="proposal-comments ">
			<% if(locals.comments) { comments.forEach(function (el, index) { %>
				<%- partial('partials/comment', el) %>
			<% }); } %>
		</div>
	</div>
</div>